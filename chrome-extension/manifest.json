{
  "manifest_version": 3,
  "name": "Undo",
  "version": "0.2",
  "description": "Warns you before sending risky messages.",
  "permissions": ["activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*","https://www.linkedin.com/*"],
      "js": ["content.js"]
    }
  ]
}
